<dsc>
		
	<storage>
		<list name="incoming_distress_calls" scope="persistant" type="net.sourceforge.dscsim.controller.network.DscMessage" key="fromMMSI"/>
		<list name="incoming_distress_acks" scope="transient"/>
		<list name="incoming_other_calls" scope="persistant" type="net.sourceforge.dscsim.controller.network.DscMessage" key="fromMMSI"/>
		<list name="mmsi_addressbook" scope="persistant" type="net.sourceforge.dscsim.controller.AddressIdEntry" key="id"/>
		<list name="group_addressbook" scope="persistant" type="net.sourceforge.dscsim.controller.AddressIdEntry" key="id"/>
	</storage>
		
	<contexts>
		<context name="individual_call" class="net.sourceforge.dscsim.controller.network.DscMessage"/>
		<context name="distress_call" class="net.sourceforge.dscsim.controller.network.DscMessage"/>
		<context name="distress_settings" class="net.sourceforge.dscsim.controller.network.DscMessage"/>		
	</contexts>
	
	<settings>
		<setting property="Time" type="net.sourceforge.dscsim.controller.screen.types.Time"/>
		<setting property="position" type="net.sourceforge.dscsim.controller.screen.types.Position"/>
		<setting property="channel" type="net.sourceforge.dscsim.controller.screen.types.Channel"/>
		<setting property="Latitude" type="net.sourceforge.dscsim.controller.screen.types.Latitude"/>
		<setting property="Longitude" type="net.sourceforge.dscsim.controller.screen.types.Longitude"/>	
		<setting property="Nature" type="net.sourceforge.dscsim.controller.screen.types.DistressNature"/>	
		<setting property="AutoAnswer" type="net.sourceforge.dscsim.controller.screen.types.DscBoolean"/>	
		<setting property="PositionType" type="net.sourceforge.dscsim.controller.screen.types.PositionType"/>					
	</settings>
			
	<incoming_call_mappings>
		<mapping call_type="TYPE_ALL_SHIPS">incoming_allships_message</mapping>
		<mapping call_type="TYPE_DISTRESS">incoming_distress_message_alarm</mapping>
		<mapping call_type="TYPE_DISTRESS_ACK">incoming_distress_message_ack</mapping>
		<mapping call_type="TYPE_INDIVIDUAL">incoming_individual_message</mapping>
		<mapping call_type="TYPE_INDIVIDUAL_ACK">incoming_individual_message_ack</mapping>	
		<mapping call_type="TYPE_GROUP">incoming_group_message</mapping>
		<mapping call_type="TYPE_GROUP_ACK">incoming_group_message_ack</mapping>	
	</incoming_call_mappings>
	

	<screens>
		<screen name="welcome"  class="net.sourceforge.dscsim.controller.screen.BeanSimpleScreen" event="FK_CALL"  link="main_entry_screen"  refresh="true" scope="transient">
			<line type="text" subtype="header" column="45">
				<bean>-VHF DSC CONTROLLER-</bean>
			</line>			
			<line>
				<bean> </bean>
			</line>
						
			<line >
				<bean>         </bean>
				<bean blink="false">CH70 WATCHING</bean>
			</line>
					
			<line column="40">
				<bean property="PositionType" class="net.sourceforge.dscsim.controller.screen.types.PositionType" mode="readonly"></bean>			
				<bean property="Time" class="net.sourceforge.dscsim.controller.screen.types.Time" mode="readonly"></bean>
			</line>
			
			<line column="40">
				<bean property="Latitude" class="net.sourceforge.dscsim.controller.screen.types.Latitude" mode="readonly"></bean>
			</line>
			
			<line column="40">
				<bean property="Longitude" class="net.sourceforge.dscsim.controller.screen.types.Longitude" mode="readonly"></bean>
			</line>	
	
			<line subtype="footer" column="45">
				<bean></bean>
			</line>
		</screen>

	  	<screen name="main_menu" class="net.sourceforge.dscsim.controller.screen.BeanSetupSendMenuScreen" scope="transient">
	  		<line subtype="header" column="45">
	  			<bean>Select a subject</bean>
	  		</line>	
	  			  		
			<line type="choice" link="position_input" condition="GPS"><bean>Entry position/time</bean></line>
			<line type="choice" link="select_call_id" code="TYPE_INDIVIDUAL"><bean>Individual call</bean></line>			
			<line type="choice" link="select_ack_quick"><bean>Individual ACK</bean></line>
			<line type="choice" link="select_group_id" code="TYPE_GROUP"><bean>Group call</bean></line>
			<line type="choice" link="select_category" code="TYPE_ALL_SHIPS"><bean>All ship calls</bean></line>			
			<line type="choice" link="posreq_select_call_id" code="POSITION_REQ"><bean>Position request</bean></line>
			<line type="choice" link="select_message"><bean>Received calls</bean></line>
			<line type="choice" link="select_distress_nature"><bean>Distress setting</bean></line>
			<line type="choice" link="setup_menu"><bean>Set up</bean></line>
					
			<line subtype="footer" column="45">
				<bean>CLR->Exit / ENT->OK</bean>
			</line>			
			
			<actions>
				<action type="exit" subtype="choice"  set="setCallType" context="individual_call"></action>
			</actions>
	
		</screen>			

		
	  	<screen name="select_call_id_old" class="net.sourceforge.dscsim.controller.screen.BeanSetupSendMenuScreen">
	  		<line subtype="header" column="45">
	  			<bean>Select address ID</bean>
	  		</line>	
	  		 		
	  		<line type="choice" link="enter_address" column="5">
	  			<bean>Manual entry</bean>
	  		</line>		  		
	  		
	  		<list storage="mmsi_addressbook">
				<line type="choice" link="select_category" storage="mmsi_addressbook" column="5">
					<bean attribute="Name"></bean>
					
					<actions>
						<action type="exit" subtype="extract" get="getId" set="setToMMSI" context="individual_call"></action>
					</actions>
			    </line>
		    </list>
			<line subtype="footer" column="45">
				<bean>CLR->Exit / ENT->OK</bean>
			</line>		
			
		</screen>			
		
		<screen name="posreq_select_call_id" class="net.sourceforge.dscsim.controller.screen.BeanSetupSendMenuScreen" scope="transient">
	  		<line subtype="header" column="45">
	  			<bean>Select address ID</bean>
	  		</line>		  		
	  		
	  		<line type="choice" link="posreq_enter_address">
	  			<bean>Manual entry</bean>
	  		</line>		  		
	  		
	  		<list storage="mmsi_addressbook">
				<line type="choice" link="individual_send" storage="mmsi_addressbook">
					<bean attribute="Name"></bean>
					
					<actions>
						<action type="exit" subtype="extract" get="getId" set="setToMMSI" context="individual_call"></action>
					</actions>
			    </line>
		    </list>
			<line subtype="footer" column="45">
				<bean>CLR->Exit / ENT->OK</bean>
			</line>		
					
		</screen>			
		
		<screen name="select_group_id" class="net.sourceforge.dscsim.controller.screen.BeanSetupSendMenuScreen">
	  		<line subtype="header" column="45">
	  			<bean>Select group ID</bean>
	  		</line>		  		
	  		
	  		<line type="choice" link="enter_address">
	  			<bean>Manual entry</bean>
	  		</line>		  		
	  		
	  		<list storage="group_addressbook">
				<line type="choice" link="select_category" storage="group_addressbook">
					<bean attribute="Name"></bean>
					
					<actions>
						<action type="exit" subtype="extract" get="getId" set="setToMMSI"  context="individual_call"/>
					</actions>
			    </line>
		    </list>
			<line subtype="footer" column="45">
				<bean>CLR->Exit / ENT->OK</bean>
			</line>		
			
		</screen>	
		
		<screen name="select_category" class="net.sourceforge.dscsim.controller.screen.BeanSetupSendMenuScreen" scope="transient">
			
			<line type="text" subtype="header" column="45"><bean>Select a category</bean></line>			
			<line type="choice" link="channel_input" code="CAT_ROUTINE"><bean>Routine</bean></line>
			<line type="choice" link="channel_input" code="CAT_SAFETY"><bean>Saftey</bean></line>
			<line type="choice" link="channel_input" code="CAT_URGENCY"><bean>Urgency</bean></line>
			<line type="choice" link="channel_input" code="CAT_DISTRESS"><bean>Distress</bean></line>
			<line type="text" link=""><bean></bean></line>		
				
			<line type="text" subtype="footer" column="45"><bean>CLR->Exit / ENT->OK</bean></line>
		
			<actions>
				<action type="exit" subtype="choice"  set="setCatagory" context="individual_call"/>
			    <action type="exit" subtype="property" property="Latitude" set="setDistressLatitude" context="individual_call"/>
			    <action type="exit" subtype="property" property="Longitude" set="setDistressLongitude" context="individual_call"/>
			    <action type="exit" subtype="property" property="Time" set="setTime" context="individual_call"/>			     			     
				<!--<mapping type="constant" class="net.sourceforge.dscsim.controller.Constants" member="CALL_TYPE_GROUP" set="setCallType" context="individual_call"/>-->
			</actions>
									
		</screen>
		

	  	<screen name="channel_input" class="net.sourceforge.dscsim.controller.screen.BeanSetupSendEntryScreen" next="individual_send" scope="transient">
	  		<line type="text" subtype="header" column="45"><bean>Input a traffic channel</bean></line>	
			
			<line>
				<bean></bean>
			</line>	
			<line>	
				<bean>CH: </bean>	
				<bean attribute="Channel" class="net.sourceforge.dscsim.controller.screen.types.Channel">16</bean>
			</line>	
						
			<line type="text" subtype="footer" column="45"><bean>CLR->Exit / ENT->OK</bean></line>

			<context name="individual_call">
				<mapping type="extract" get="getChannel" set="setChannel"></mapping>
			</context>
							
		</screen>
		
		<screen name="enter_address" class="net.sourceforge.dscsim.controller.screen.BeanSetupSendEntryScreen" next="select_category" scope="transient">
	  		<line type="text" subtype="header" column="45"><bean>Input an address</bean></line>	
			
			<line>
				<bean></bean>
			</line>	
			
			<line>	
				<bean>ID:(9 digit)</bean>	
				<bean attribute="ToMMSI" class="net.sourceforge.dscsim.controller.screen.types.MMSI"></bean>
			</line>	
						
			<line type="text" subtype="footer" column="45"><bean>CLR->Exit / ENT->OK</bean></line>

			<context name="individual_call">
				<mapping type="extract" get="getToMMSI" set="setToMMSI"></mapping>
			</context>
							
		</screen>
				
		<screen name="individual_send" class="net.sourceforge.dscsim.controller.screen.BeanSendScreen">
			<line type="text" subtype="header" column="45"><bean>Push [CALL] and [ENT]</bean></line>
			<line type="text" subtype="header" column="45"><bean>simultaneously</bean></line>						

			<line type="text" subtype="footer" column="45"><bean>To stop the call,</bean></line>
			<line type="text" subtype="footer" column="45"><bean>push [CLR]</bean></line>	
			
			<context name="individual_call">
			</context>

		</screen>	
		
		<screen name="distress_call_send" type="action" class="net.sourceforge.dscsim.controller.screen.SendDistressScreen">
			<line type="text" subtype="header" column="0"><bean>Push and hold</bean></line>
			<line type="text" subtype="header" column="0"><bean>[DISTRESS] for 5 sec</bean></line>						

			<line column="40">
				<bean>Nature of distress</bean>
			</line>	

			<line column="40">
				<bean property="Nature" class="net.sourceforge.dscsim.controller.screen.types.DistressNature" mode="readonly"></bean>		
			</line>
			
			<line column="40">
				<bean property="PositionType" class="net.sourceforge.dscsim.controller.screen.types.PositionType" mode="readonly"></bean>
				<bean property="Time" class="net.sourceforge.dscsim.controller.screen.types.Time" mode="readonly"></bean>					
			</line>			
				
			<line column="40">
				<bean property="Latitude" class="net.sourceforge.dscsim.controller.screen.types.Latitude" mode="readonly"></bean>			
			</line>
			
			<line column="40">
				<bean property="Longitude" class="net.sourceforge.dscsim.controller.screen.types.Longitude" mode="readonly"></bean>			
			</line>	
			<!--
			<line type="text" subtype="footer" column="45">
				<bean>[CLR]->ACK</bean>
			</line>		
			-->		
								
		</screen>

		<screen name="distress_call_sent"  class="net.sourceforge.dscsim.controller.screen.BeanSimpleScreen" event="FK_CALL"  link="main_menu"  refresh="true" scope="transient">
		
			<line subtype="header" column="0">
				<bean>Distress alert</bean>
			</line>
			<line subtype="header" column="45">
				<bean>Completed</bean>
			</line>			
						
			<line><bean></bean></line>
			
			<line>
				<bean blink="true">Now waiting for</bean>
			</line>	
							
			<line column="40">
				<bean blink="true">acknowledgement</bean>
			</line>	
			

			<line subtype="footer" column="45">
				<bean>[CLR]->Exit</bean>
			</line>	
														
		</screen>
						
		<screen name="send_confirmation" type="input">
			<line type="output">			
				<field type="text">Send Status:</field>
				<field type="output" name="STATUS"></field>
			</line>			
		</screen>

	
	  <screen name="position_input_old" class="net.sourceforge.dscsim.controller.screen.BeanPropertyScreenItem" next="time_input" >
	  
			<line type="text" subtype="header" column="45">
				<bean>Position input</bean>
			</line>		
					
			<line>
				<bean> </bean>
			</line>
			
			<line column="5">			
				<bean property="Latitude" class="net.sourceforge.dscsim.controller.screen.types.Latitude"></bean>
			</line>		

			<line column="5">			
				<bean property="Longitude" class="net.sourceforge.dscsim.controller.screen.types.Longitude"></bean>
			</line>		

			<line type="text" subtype="footer" column="45">
				<bean>BS->Null data</bean>
			</line>						
			<line type="text" subtype="footer" column="45">
				<bean>CLR->Exit / ENT->OK</bean>
			</line>		
		</screen>
		
		<screen name="position_input" clazz="net.sourceforge.dscsim.controller.screen.impl.PositionEntryScreen">

			<element type="textbox" name="header" column="2" row="0" height="1" width="18">Position input</element>	
		
			<element type="textbox" name="label1" column="1" row="2" height="1" width="9">Lattitude</element>				
			<element type="editbox" name="latdeg" column="7" row="3" height="1" width="2"></element>	
			<element type="editbox" name="latmin" column="10" row="3" height="1" width="2"></element>	

			<element type="textbox" name="label2" column="1" row="4" height="1" width="9">Longitude</element>				
			<element type="editbox" name="londeg" column="6" row="5" height="1" width="3"></element>	
			<element type="editbox" name="lonmin" column="10" row="5" height="1" width="2"></element>	
	
			<element type="textbox" name="footer" column="2" row="6" height="1" width="17">BS->Null data</element>	
			<element type="textbox" name="footer" column="2" row="7" height="1" width="15">CLR->Exit / ENT->OK</element>	
					
			<actions>
				<event id="FK_ENT" action="setup_address_info"/>
				<!--<event id="FK_CLR" action="welcome"/>-->
			</actions>			
		</screen>		
	
		  <screen name="time_input" class="net.sourceforge.dscsim.controller.screen.BeanPropertyScreenItem" next="welcome" >
	  
			<line type="text" subtype="header" column="45">
				<bean>Input a Time</bean>
			</line>		

			<line>			
				<bean> </bean>
			</line>		
					
			<line  column="5">			
				<bean property="Time" class="net.sourceforge.dscsim.controller.screen.types.Time"></bean>
			</line>		

			<line type="text" subtype="footer" column="45">
				<bean>BS->Null data</bean>
			</line>						

			<line type="text" subtype="footer" column="45">
				<bean>CLR->Exit / ENT->OK</bean>
			</line>		
		</screen>
		
		<screen name="system_error" type="input">
			<line type="text">System Error</line>		
			<line type="text">Press Clear</line>				
		</screen>
		
		<screen name="system_not_implemented" type="input">
			<line type="text" subtype="header" column="45">Implementation not complete</line>
			<line type="text" subtype="footer" column="45">push [CLR]</line>			
		</screen>
								
		<screen name="select_message" class="net.sourceforge.dscsim.controller.screen.BeanMenuScreen" scope="transient">
		
			<line subtype="header" column="45">
				<bean>Select a message</bean>
			</line>	
			
			<line type="choice" link="show_distress_calls" column="5">
				<bean>Distress message</bean>
			</line>
			
			<line type="choice" link="show_other_calls" column="5">
				<bean>Other message</bean>
			</line>
			
			<line type="text" subtype="footer" column="45">
				<bean>CLR->Exit / ENT->OK</bean>
			</line>
		</screen>
					
		<screen name="show_distress_calls"  class="net.sourceforge.dscsim.controller.screen.BeanMenuScreen">
			<line type="text" subtype="header" column="45">
				<bean>Distress Calls</bean>
			</line>					
				
			<list storage="incoming_distress_calls" numbered="true">
				<line type="choice" column="5" link="show_distress_call" storage="incoming_distress_calls">
				    <bean>  </bean>					
					<bean attribute="CallTypeText"></bean>
					<bean>  </bean>
					<bean attribute="Time"></bean>	
			    </line>
		    </list>
		    
			<line subtype="footer" column="45">
				<bean>CLR->Exit / ENT->OK</bean>
			</line>
		</screen>
		
		<screen name="show_distress_call" class="net.sourceforge.dscsim.controller.screen.BeanMenuScreenItem" mode="view" storage="incoming_distress_calls" scroll="false">
	  		<line subtype="header" column="45">
	  			<bean>Distress call</bean>
	  		</line>	
		
			<line column="5">		
				<bean>Distress ID:</bean>
				<bean attribute="FromMMSI" format="#########"></bean>
			</line>
										
			<line column="5">		
				<bean attribute="Time"></bean>
				<bean> </bean>	
			    <bean>CH:</bean>
				<bean attribute="Channel" format="##"></bean>										
			</line>
			
			<line column="5">		
				<bean attribute="Latitude"></bean>
			</line>
			<line column="5">			
				<bean attribute="Longitude"></bean>
			</line>			
						
			<line column="5">	
			    <bean>nature:</bean>		
				<bean attribute="NatureText"></bean>			
			</line>	
								
			<line subtype="footer" column="45">
				<bean>CLR->Exit / ENT->OK</bean>
			</line>			
		</screen>

		<screen name="show_other_calls"  class="net.sourceforge.dscsim.controller.screen.BeanMenuScreen">
			<line type="text" subtype="header" column="45">
				<bean>Other Calls</bean>
			</line>					
				
			<list storage="incoming_other_calls" numbered="true">
				<line type="choice" column="5" link="show_other_call_detail" storage="incoming_other_calls">
					<bean>  </bean>				
					<bean attribute="CallTypeText"></bean>
			    </line>
		    </list>
		    
			<line subtype="footer" column="45">
				<bean>CLR->Exit / ENT->OK</bean>
			</line>
		</screen>
		
		<screen name="show_other_call_detail" class="net.sourceforge.dscsim.controller.screen.BeanMenuScreenItem" mode="view" storage="incoming_other_calls" scroll="false">
	  		<line subtype="header" column="45">
	  			<bean>Other call</bean>
	  		</line>	

			<line column="5">		
				<bean attribute="CatagoryForTypeText"></bean>	
			</line>
					
			<line column="5">		
				<bean>From: </bean>
				<bean attribute="FromMMSI" format="#########"></bean>
			</line>
					
			<line column="5">		
				<bean>F3E/G3E simplex</bean>
			</line>
				
			<line column="5">	
			    <bean>CH:</bean>
				<bean attribute="Channel" format="##"></bean>		
			</line>
				
			<line subtype="footer" column="45">
				<bean>CLR->Exit / ENT->OK</bean>
			</line>			
		</screen>
							
		<screen name="incoming_distress_message_alarm"  class="net.sourceforge.dscsim.controller.screen.BeanSimpleScreen" event="FK_CLR"  link="incoming_distress_message_quiet" scope="transient" >
				
			<line type="text" subtype="header" column="45">
				<bean>-RCV Distress-</bean>
			</line>
			
			<line column="5">	
			 	<bean>Distress ID: </bean>			
				<bean attribute="FromMMSI"></bean>			
			</line>	
			
			<line column="5">	
			    <bean>nature:</bean>		
				<bean attribute="NatureText"></bean>			
			</line>			
														
			<line subtype="footer" column="45">
				<bean>[CLR] - Alarm stop</bean>
			</line>	
			
			<data name="incoming_distress_calls" type="list" index="last"/>
			
		</screen>
				
		<screen name="incoming_distress_message_quiet"  class="net.sourceforge.dscsim.controller.screen.BeanSimpleScreen" event="FK_CLR"  link="welcome" scope="transient" >
				
			<line type="text" subtype="header" column="45">
				<bean>-RCV Distress-</bean>
			</line>
			
			<line column="5">	
			 	<bean>Distress ID:</bean>			
				<bean attribute="FromMMSI"></bean>			
			</line>			

			<line column="5">	
			    <bean>Time: </bean>		
				<bean attribute="DistressTime"></bean>			
			</line>			

			<line column="5">			
				<bean attribute="DistressLatitude"></bean>			
			</line>			

			<line column="5">			
				<bean attribute="DistressLongitude"></bean>			
			</line>	
			
			<line column="5">	
			    <bean>nature:</bean>		
				<bean attribute="NatureText"></bean>			
			</line>			
					
																	
			<line subtype="footer" column="45">
				<bean>[CLR] - Exit</bean>
			</line>	
			
			<data name="incoming_distress_calls" type="list" index="last"/>
			
		</screen>

		<screen name="incoming_individual_message"  class="net.sourceforge.dscsim.controller.screen.SetupOutgoingDscMessageScreen" mode="menu" event="FK_ENT"  link="ack_individual_compliance" scope="transient" >
					
			<line type="text" subtype="header" column="45">
				<bean>-RCV Individual call-</bean>
			</line>	

			<line type="choice" link="select_ack_quick">		
				<bean></bean>			
			</line>	

			<line column="5">	
			     <bean>From: </bean>
				<bean attribute="FromMMSI"></bean>			
			</line>	
										
			<line type="choice" link="select_ack_quick">		
				<bean></bean>			
			</line>	
																				
			<line type="text" subtype="footer" column="45">
				<bean>[CLR]->Exit / [ENT]->OK</bean>
			</line>
						
			<context name="individual_call">
				<mapping type="constant" class="net.sourceforge.dscsim.controller.Constants" member="CALL_TYPE_INDIVIDUAL_ACK" set="setCallType"/>		    		
			</context>
						
		</screen>
	
		<screen name="ack_individual_select_address" class="net.sourceforge.dscsim.controller.screen.SetupOutgoingDscMessageScreen" mode="menu">
			
			<line subtype="header" column="45">
				<bean>-RCV Individual Call-</bean>
			</line>
			
			<line subtype="header" column="5">
				<bean>Select address ID</bean>
			</line>
			
