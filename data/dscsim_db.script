CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE DSCMESSAGE(UID TIMESTAMP NOT NULL,SENDER CHAR(9) NOT NULL,RECIPIENT CHAR(9),CALL_TYPE_CD CHAR(32),NATURE_CD CHAR(32),CATAGORY_CD CHAR(32),POS_LAT_DEG INTEGER,POS_LAT_MIN INTEGER,POS_LAT_HEM CHAR(1),POS_LON_DEG INTEGER,POS_LON_MIN INTEGER,POS_LON_HEM CHAR(1),POS_TIME_HRS INTEGER,POS_TIME_MIN INTEGER,CHANNEL INTEGER,COMPLIANCE_CD CHAR(32),COMPLIANCE_REASON_CD CHAR(32),ACKD_TIME TIMESTAMP,PRIMARY KEY(UID,SENDER))
CREATE MEMORY TABLE ADDRESS_BOOK(ID CHAR(9) NOT NULL PRIMARY KEY,NAME CHAR(15),TYPE_CD CHAR(2))
CREATE MEMORY TABLE DISTRESS_SETTINGS(SENDER CHAR(9) NOT NULL PRIMARY KEY,NATURE_CD CHAR(32),POS_LAT_DEG INTEGER,POS_LAT_MIN INTEGER,POS_LAT_HEM CHAR(1),POS_LON_DEG INTEGER,POS_LON_MIN INTEGER,POS_LON_HEM CHAR(1),POS_TIME_HRS INTEGER,POS_TIME_MIN INTEGER,ENTER_TIME TIMESTAMP)
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 1
SET SCHEMA PUBLIC

